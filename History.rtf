\f2\b0\fs26 \cf10 \cb6 interface\cf5  \cf11 VoiceChatConfig\cf5  \{\
  pauseThreshold\cf12 :\cf5  \cf13 number\cf5 ;      
\f7\i \cf14 // 2000-3000ms silence = send
\f2\i0 \cf5 \
  useCamera\cf12 :\cf5  \cf13 boolean\cf5 ;          
\f7\i \cf14 // Emotion detection enabled
\f2\i0 \cf5 \
  speakResponses\cf12 :\cf5  \cf13 boolean\cf5 ;     
\f7\i \cf14 // TTS for responses
\f2\i0 \cf5 \
  showTranscript\cf12 :\cf5  \cf13 boolean\cf5 ;     
\f7\i \cf14 // Display text while talking
\f2\i0 \cf5 \
\}\
\
\pard\pardeftab720\partightenfactor0

\f7\i \cf14 // Speech-to-text with pause detection
\f2\i0 \cf5 \
\pard\pardeftab720\partightenfactor0
\cf10 const\cf5  recognition \cf12 =\cf5  \cf10 new\cf5  \cf11 SpeechRecognition\cf5 ();\
recognition.continuous \cf12 =\cf5  \cf11 true\cf5 ;\
recognition.interimResults \cf12 =\cf5  \cf11 true\cf5 ;\
\
\cf10 let\cf5  silenceTimer\cf12 :\cf5  NodeJS.Timeout;\
\
recognition.\cf12 onresult\cf5  \cf12 =\cf5  (event) \cf12 =>\cf5  \{\
  \cf12 clearTimeout\cf5 (silenceTimer);\
  \
  
\f7\i \cf14 // Reset timer on speech
\f2\i0 \cf5 \
  silenceTimer \cf12 =\cf5  \cf12 setTimeout\cf5 (() \cf12 =>\cf5  \{\
    
\f7\i \cf14 // Pause detected - process message
\f2\i0 \cf5 \
    \cf10 const\cf5  transcript \cf12 =\cf5  \cf12 getFinalTranscript\cf5 (event);\
    \cf10 const\cf5  emotion \cf12 =\cf5  currentEmotionState; 
\f7\i \cf14 // from camera
\f2\i0 \cf5 \
    \cf12 sendToCoach\cf5 (transcript, emotion);\
  \}, config.pauseThreshold);\
\};\
